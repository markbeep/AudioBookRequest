{#def
    user: DetailedUser,
    title: str,
    page: str,
    counts: WishlistCounts,
#}

<BaseLayout title={{ title }} user={{ user }}>
<div class="w-screen p-2 md:p-4 lg:p-8 gap-2">
    <h1 class="text-3xl font-bold">
        Wishlist
    </h1>

    <Wishlist.Tablist page={{ page }} counts={{ counts }} />

    {{ content }}
</div>
</BaseLayout>
